{
  "name": "evoluty/google-maps-client",
  "description": "A PHP Google Maps APIs client",
  "keywords": ["google", "map", "client"],
  "license": "MIT",
  "authors": [
    {
      "name": "Franck Schmitt",
      "email": "franck.schmitt@hotmail.com"
    }
  ],
  "require": {
    "php": ">=7.2",
    "ext-json": "*",
    "psr/http-client": "^1.0",
    "php-http/discovery": "^1.6",
    "psr/http-factory": "^1.0",
    "myclabs/php-enum": "^1.6"
  },
  "require-dev": {
    "phpunit/phpunit": "^5",
    "guzzlehttp/guzzle": "^6.3",
    "php-http/guzzle6-adapter": "^2.0",
    "http-interop/http-factory-guzzle": "^1.0",
    "zendframework/zend-diactoros": "^2.1"
  },
  "suggest": {
    "guzzlehttp/guzzle": "^6.3",
    "php-http/guzzle6-adapter": "^2.0",
    "zendframework/zend-diactoros": "^2.1"
  },
  "autoload": {
    "psr-4": {
      "GoogleMapsClient\\": "src/"
    }
  },
  "scripts": {
    "test": "phpunit",
    "local-test": "phpunit --coverage-text"
  },
  "autoload-dev": {
    "psr-4": {
      "GoogleMapsClient\\Tests\\": "tests/"
    }
  }
}
